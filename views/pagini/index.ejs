            <div id="grid-galerie">
                <%
                 let count = 0; 
                for (let imag of locals.imagini) { 
                    d=new Date()
                    var anotimpuri=["iarna","iarna","primavara","primavara","primavara","vara","vara","vara","toamna","toamna","toamna","iarna"]
                    if(imag.anotimp && imag.anotimp.includes(anotimpuri[d.getMonth()])){
                        if (count >= 13) { break; }
                        count++; // increment counter
                %>
                        
                        <figure class ="grid-item">
                            <picture>
                                <source srcset="<%- imag.fisier_mic %>" media="(max-width:700px)" />
                                <source srcset="<%- imag.fisier_mediu %>" media="(max-width:1000px)" />
                                <img src="<%- imag.fisier %>" alt="imagine">
